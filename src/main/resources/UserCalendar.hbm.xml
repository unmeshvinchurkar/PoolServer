<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pool.spring.model.UserCalendarDay" table="user_calender">
		<id name="userCalendarDayId" type="java.lang.Long">
			<column name="calendar_id" />
			<generator class="increment" />
		</id>
		<property name="calendarDay" type="java.lang.Long">
			<column name="calendar_day" />
		</property>
		<property name="userId" type="java.lang.Long">
			<column name="user_id" />
		</property>
		<property name="carPoolId" type="java.lang.Long">
			<column name="car_pool_id" />
		</property>
	</class>
</hibernate-mapping>
