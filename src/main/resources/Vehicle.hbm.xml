<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.pool.spring.model.Vehicle" table="vehicle">

		<id name="vehicleId" type="long">
			<column name="VehicleId" />
			<generator class="increment" />
		</id>
		<property name="ownerId" type="java.lang.Long">
			<column name="owner_id" />
		</property>
		<property name="registrationNo" type="string">
			<column name="registration_no" />
		</property>
		<property name="vehicleType" type="string">
			<column name="vehicle_type" />
		</property>
		<property name="fuelType" type="string">
			<column name="fuel_type" />
		</property>
		<property name="color" type="string">
			<column name="color" />
		</property>
		<property name="manufacturer" type="string">
			<column name="manufacturer" />
		</property>
		<property name="model" type="string">
			<column name="model" />
		</property>
		<property name="noOfSeats" type="int">
			<column name="no_of_seats" />
		</property>
	</class>
</hibernate-mapping>
