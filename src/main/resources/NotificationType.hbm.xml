<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pool.spring.model.NotificationType" table="notification_type">

		<id name="notificationTypeId" type="java.lang.Long">
			<column name="notification_type_id" length="100" not-null="true" />
			<generator class="increment" />
		</id>

		<property name="notificationType" type="string">
			<column name="notification_type" length="100" />
		</property>
	</class>
</hibernate-mapping>
